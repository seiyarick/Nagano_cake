<div class="container">
  <h4 class="my-4">ジャンル編集</h4>
  <div class="row">
    <div class="form_group">
      <%= form_with model:@genre ,url: admin_genre_path do |f| %><!--[PATCH]
      updateのpathが必要-->
      <label class="genre_name mr-5">ジャンル名</label>
      <%= f.text_field :name, class:"mr-5" %>
      <%= f.submit "変更を保存", class:"btn btn-success" %>
      <% end %>
    </div>
  </div>
</div>

